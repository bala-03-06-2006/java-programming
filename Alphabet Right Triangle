import java.util.*;
public class Alphabet {
    public static void main(String[] args) {
                Scanner mcu = new Scanner(System.in);
                int n=mcu.nextInt();
        for (int i=0;i<n;i++) {
            char ch=(char)('A'+i);
            for (int j=0;j<=i;j++) {
                System.out.print((char)(ch+j)+" ");
            }
            
            System.out.println();
        }
    }
}
