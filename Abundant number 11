import java.util.*;
public class Abundant {
    public static void main(String[] args) {
        Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
        int s=0;
        for (int i=1;i<a;i++){
            if(a%i==0){
                s+=i;
            }
    }
    if(s>a)
    {
        System.out.println("Abundant Number");
    }
    else{
        System.out.println("Not Abundant Number");
    }
}
}
